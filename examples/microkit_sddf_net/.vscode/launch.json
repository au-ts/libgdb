    "version": "0.2.0",
    "configurations": [
    {
        "type": "gdb",
        "request": "attach",
        "gdbpath": "aarch64-none-elf-gdb",
        "name": "Attach to gdbserver",
        "executable": "./build/ping.elf",
        "target": ":1234",
        "remote": true,
        "cwd": "${workspaceRoot}",
        "valuesFormatting": "parseText",
        "stopAtConnect": true,
    },
